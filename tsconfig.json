{
  "compilerOptions": {
    "outDir": "./public/",
    "noImplicitAny": true,
    "module": "es2020",
    "target": "es2020",
    "strict": true,
    "strictFunctionTypes": false,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
